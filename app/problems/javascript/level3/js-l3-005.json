    "function memoize(fn) {",
    "  const cache = {};",
    "  return function(...args) {",
    "    const key = args;",
    "    if (cache[key]) {",
    "      return cache[key];",
    "    }",
    "    const result = fn(...args);",
    "    cache[key] = result;",
    "    return result;",
    "  };",
    "}"
